{
  "app": {
    "bundleName": "com.example.distschedule",
    "vendor": "example",
    "version": {
      "code": 1000000,
      "name": "1.0.0"
    }
  },
  "deviceConfig": {},
  "module": {
    "package": "com.example.netconfig",
    "name": ".MyApplication",
    "mainAbility": "com.example.netconfig.MainAbility",
    "deviceType": [
      "phone"
    ],
    "distro": {
      "deliveryWithInstall": true,
      "moduleName": "netconfig",
      "moduleType": "feature",
      "installationFree": false
    },
    "metaData": {
      "customizeData": [
        {
          "name": "hwc-theme",
          "value": "androidhwext:style/Theme.Emui.NoTitleBar",
          "extra": ""
        }
      ]
    },
    "abilities": [
      {
        "visible": true,
        "name": "com.example.netconfig.MainAbility",
        "icon": "$media:icon",
        "description": "$string:mainability_description",
        "label": "$string:netconfig_MainAbility",
        "type": "page",
        "launchType": "standard"
      }
    ],
    "js": [
      {
        "pages": [
          "pages/index/index"
        ],
        "name": "default",
        "window": {
          "designWidth": 720,
          "autoDesignWidth": true
        }
      }
    ]
  }
}